# Environment Variables for Autonomous Domjuan Bot System
# WARNING: Do NOT put actual secrets in this file. Use it for reference only.

# Basic Configuration
NODE_ENV=production
PORT=3000

# Original Domain/Payment System (if needed)
SESSION_SECRET="your_session_secret_here"
STRIPE_SECRET_KEY="your_stripe_secret_key_here"
STRIPE_PUBLISHABLE_KEY="your_stripe_publishable_key_here"
STRIPE_WEBHOOK_SECRET="your_stripe_webhook_secret_here"

# Autonomous Bot System Configuration
AUTO_RESTART_BOTS=true
EXPORT_INTERVAL_MS=300000
MAX_CONSECUTIVE_ERRORS=5
DATA_DIR=./data
MAX_LOG_AGE_MS=86400000

# Webhook Notifications (Discord/Slack)
WEBHOOK_URL="https://discord.com/api/webhooks/your_webhook_url"

# Monitoring Configuration
HEALTH_CHECK_INTERVAL_MS=60000
MIN_FREE_DISK_MB=100
MAX_MEMORY_MB=400
MAX_CPU_PERCENT=80

# Database Configuration (if using external DB)
DATABASE_URL="your_database_url_here"
PGDATABASE="your_pgdatabase_here"
PGHOST="your_pghost_here"
PGPORT="your_pgport_here"
PGUSER="your_pguser_here"
PGPASSWORD="your_pgpassword_here"

# GoDaddy API Configuration
GODADDY_API_KEY="your_godaddy_api_key_here"
GODADDY_API_SECRET="your_godaddy_api_secret_here"

# Namecheap API Configuration
NAMECHEAP_API_USER="your_namecheap_api_user_here"
NAMECHEAP_API_KEY="your_namecheap_api_key_here"
NAMECHEAP_USERNAME="your_namecheap_username_here"
NAMECHEAP_CLIENT_IP="your_client_ip_here"

# Backup Configuration
BACKUP_DIR=./backups
RETENTION_DAYS=30
COMPRESS_BACKUPS=true

# Production Deployment Keys
HEROKU_API_KEY="your_heroku_api_key_here"
DOCKER_USERNAME="your_docker_username_here"
DOCKER_PASSWORD="your_docker_password_here"
VERCEL_TOKEN="your_vercel_token_here"

# Legacy Square Integration (if needed)
SQUARE_TERMINAL_ID="your_square_terminal_id_here"
Webhooksignaturekeydomjuan="your_webhook_signature_key_here"
