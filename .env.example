# Environment Variables for Autonomous Domain Discovery Bot
# WARNING: Do NOT put actual secrets in this file. Use it for reference only.

# Core Configuration
NODE_ENV=development
AUTO_START_BOT=false
BOT_LOG_LEVEL=info
BOT_MAX_DEPTH=5
BOT_MAX_ACQUISITION_COST=1000

# Stripe Configuration (for payment processing)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Registrar API Keys (for domain acquisition)
GODADDY_API_KEY=your_godaddy_api_key
GODADDY_API_SECRET=your_godaddy_api_secret
NAMECHEAP_API_KEY=your_namecheap_api_key
NAMECHEAP_USERNAME=your_namecheap_username
SPACESHIP_API_KEY=your_spaceship_api_key
AFTERNIC_API_KEY=your_afternic_api_key

# Database Configuration
DATABASE_URL=sqlite:./bot/data/discoveries.db
PGDATABASE=your_pgdatabase_here
PGHOST=your_pghost_here
PGPORT=your_pgport_here
PGUSER=your_pguser_here
PGPASSWORD=your_pgpassword_here

# Security & Authentication
SESSION_SECRET=your_session_secret_here
ENCRYPTION_KEY=your_32_character_encryption_key_here
API_RATE_LIMIT=100

# Webhook URLs (optional)
WEBHOOK_DISCOVERY_URL=https://your-domain.com/webhooks/discovery
WEBHOOK_ACQUISITION_URL=https://your-domain.com/webhooks/acquisition
WEBHOOK_ERROR_URL=https://your-domain.com/webhooks/error

# Legacy Configuration (maintained for compatibility)
DOMAIN_REGISTRAR_API_KEY=your_domain_registrar_api_key_here
SQUARE_TERMINAL_ID=your_square_terminal_id_here
Webhooksignaturekeydomjuan=your_webhook_signature_key_here
Chimeaccountnumber=your_chime_account_number_here
Chimeroutingnumber=your_chime_routing_number_here