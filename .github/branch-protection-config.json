{
  "branch_protection": {
    "required_status_checks": {
      "strict": true,
      "contexts": [
        "validate-and-test (18.x)",
        "validate-and-test (20.x)",
        "security-scan"
      ]
    },
    "enforce_admins": false,
    "required_pull_request_reviews": {
      "required_approving_review_count": 1,
      "dismiss_stale_reviews": true,
      "require_code_owner_reviews": false,
      "dismiss_stale_reviews": true
    },
    "restrictions": null,
    "allow_force_pushes": false,
    "allow_deletions": false,
    "required_linear_history": true
  },
  "auto_merge_criteria": {
    "trusted_bots": [
      "dependabot[bot]",
      "github-actions[bot]"
    ],
    "auto_merge_labels": [
      "auto-merge",
      "dependencies"
    ],
    "auto_merge_title_patterns": [
      "^\\[auto\\]",
      "^chore:",
      "^deps:",
      "^Bump "
    ]
  }
}